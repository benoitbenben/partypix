<div class="navbar-wagon">
  <div class="navbar-wagon-left">
    <%= link_to root_path, class: "navbar-wagon-brand" do %>
      <%= image_tag "PartyPix_Color_Logo.png" %>
    <% end %>

    <% if user_signed_in? %>
      <%= link_to rooms_path, class: "navbar-wagon-item" do %>
        <i class="fab fa-fort-awesome"></i>
      <% end %>
    <% end %>

  </div>


  <% if user_signed_in? %>

    <div class="navbar-wagon-right">

      <%= link_to profile_path, class: "navbar-wagon-item" do %>
        <i class="fas fa-user-circle"></i>
      <% end %>

      <%= link_to destroy_user_session_path, method: :delete, class: "navbar-wagon-item" do %>
        <i class="fas fa-sign-out-alt"></i>
      <% end %>

    </div>

  <% end %>

</div>
