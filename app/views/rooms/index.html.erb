<div class="background_rainbow">

  <div class="container">

    <div class="row">

      <% @yearly_grouped_rooms.keys.each do |year| %>

        <div class="col-xs-12">
          <div class="year"><%= year %></div>
        </div>

        <% @yearly_grouped_rooms[year].each do |room| %>
          <%= render "room_card", room: room  %>

        <% end %>

      <% end %>

    </div>

  </div>

</div>

<div class="bottom-menu">
    <div class="bottom-menu-flex">
      <%= link_to new_room_path, class: "bottom-menu-element bottom-flex" do %>
        <%= image_tag "rounded-add-button.svg" %>
        <span>CREATE A ROOM</span>

     <% end %>
     </div>
</div>
